{
    "name": "born",
    "title": "管理后台",
    "contextPath": "/admin",
    "runtime": {
        "skylarkjs": {
            "version": "0.9.5"
        },
        "paths": {
            "server": "scripts/services/server",
            "MediumEditor": "https://cdn.jsdelivr.net/npm/medium-editor@latest/dist/js/medium-editor.min",
            "moment": "https://cdn.bootcss.com/moment.js/2.19.3/moment-with-locales.min",
            "photoswipe": "https://cdn.bootcss.com/photoswipe/4.1.2/photoswipe.min",
            "photoswipeUi": "https://cdn.bootcss.com/photoswipe/4.1.2/photoswipe-ui-default.min",
            "skylarkBs": "lib/uncompressed/skylark-bs-swt",
            "lodash": "https://cdn.bootcss.com/lodash.js/4.17.4/lodash.min",
            "jquery": "lib/uncompressed/skylark-jquery",
            "simplemde": "https://cdn.bootcss.com/simplemde/1.11.2/simplemde.min",
            "toastr": "https://cdn.bootcss.com/toastr.js/latest/js/toastr",
            "handlebars": "https://cdn.bootcss.com/handlebars.js/4.0.11/handlebars.amd.min",
            "text": "https://cdn.bootcss.com/require-text/2.0.12/text.min"
        }
    },
    "page": {
        "routeViewer": "#main"
    },
    "spaModule": "scripts/helpers/spa",
    "plugins": {
        "app": {
            "hookers": "starting started",
            "controller": {
                "type": "scripts/plugins/app/AppController"
            }
        },
        "navbar": {
            "hookers": "starting",
            "controller": {
                "type": "scripts/plugins/navbar/NavbarController"
            }
        }
    },
    "routes": {
        "home": {
            "pathto": "/",
            "data": {
                "name": "home",
                "navName": "管理主页"
            },
            "controller": {
                "type": "scripts/routes/home/HomeController"
            }
        },
        "pages": {
            "pathto": "/pages",
            "data": {
                "name": "pages",
                "navName": "页面"
            },
            "controller": {
                "type": "scripts/routes/pages/PagesController"
            }
        },
        "processPost": {
            "pathto": "/process",
            "data": {
                "name": "processPost",
                "navName": "赴美流程"
            },
            "controller": {
                "type": "scripts/routes/posts/ProcessController"
            }
        },
        "envPost": {
            "pathto": "/enviroment",
            "data": {
                "name": "envPost",
                "navName": "待产环境"
            },
            "controller": {
                "type": "scripts/routes/posts/EnvController"
            }
        },
        "meetPost": {
            "pathto": "/meet",
            "data": {
                "name": "meetPost",
                "navName": "遇见美利坚"
            },
            "controller": {
                "type": "scripts/routes/posts/MeetController"
            }
        },
        "aboutPost": {
            "pathto": "/about",
            "data": {
                "name": "aboutPost",
                "navName": "关于我们"
            },
            "controller": {
                "type": "scripts/routes/posts/AboutController"
            }
        },
        "activityPost": {
            "pathto": "/activity",
            "data": {
                "name": "activityPost",
                "navName": "在美活动"
            },
            "controller": {
                "type": "scripts/routes/posts/ActivityController"
            }
        },
        "servicePost": {
            "pathto": "/service",
            "data": {
                "name": "servicePost",
                "navName": "服务"
            },
            "controller": {
                "type": "scripts/routes/posts/ServiceController"
            }
        },
        "photos": {
            "pathto": "/photos",
            "data": {
                "name": "photos",
                "navName": "图片"
            },
            "controller": {
                "type": "scripts/routes/photos/PhotosController"
            }
        },
        "news": {
            "pathto": "/news",
            "data": {
                "name": "news",
                "navName": "新闻"
            },
            "controller": {
                "type": "scripts/routes/news/NewsController"
            }
        },
        "slides": {
            "pathto": "/slides",
            "data": {
                "name": "slides",
                "navName": "Slide"
            },
            "controller": {
                "type": "scripts/routes/slides/SlidesController"
            }
        },
        "categories": {
            "pathto": "/categories",
            "data": {
                "name": "categories",
                "navName": "分类"
            },
            "controller": {
                "type": "scripts/routes/categories/CategoriesController"
            }
        },
        "contents": {
            "pathto": "/contents",
            "data": {
                "name": "contents",
                "navName": "页面内容"
            },
            "controller": {
                "type": "scripts/routes/contents/ContentsController"
            }
        },
        "qas": {
            "pathto": "/qas",
            "data": {
                "name": "qas",
                "navName": "QA"
            },
            "controller": {
                "type": "scripts/routes/qas/QasController"
            }
        },
        "snippets": {
            "pathto": "/snippets",
            "data": {
                "name": "snippets",
                "navName": "段内容"
            },
            "controller": {
                "type": "scripts/routes/snippets/SnippetsController"
            }
        },
        "links": {
            "pathto": "/links",
            "data": {
                "name": "links",
                "navName": "链接"
            },
            "controller": {
                "type": "scripts/routes/links/LinksController"
            }
        }
    }
}