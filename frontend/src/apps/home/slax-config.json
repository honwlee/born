{
    "name": "born",
    "title": "BornInUSA·生美国际-中美直营的赴美生子咨询服务商-赴美生子-南京赴美生子-赴美生子机构-赴美生子月子中心",
    "runtime": {
        "skylarkjs": {
            "version": "0.9.5"
        },
        "paths": {
            "server": "scripts/services/server",
            "skylarkBs": "lib/skylark-bs-swt-all",
            "jquery": "lib/skylark-jquery",
            "toastr": "https://cdn.bootcss.com/toastr.js/latest/js/toastr.min",
            "bootstrap": "https://cdn.bootcss.com/bootstrap/3.3.6/js/bootstrap.min",
            "handlebars": "https://cdn.bootcss.com/handlebars.js/4.0.11/handlebars.amd.min",
            "text": "https://cdn.bootcss.com/require-text/2.0.12/text.min"
        }
    },
    "page": {
        "routeViewer": "#main"
    },
    "spaModule": "scripts/helpers/spa",
    "plugins": {
        "app": {
            "hookers": "starting started",
            "controller": {
                "type": "scripts/plugins/app/AppController"
            }
        },
        "navbar": {
            "hookers": "starting",
            "controller": {
                "type": "scripts/plugins/navbar/NavbarController"
            }
        }
    },
    "routes": {
        "home": {
            "pathto": "/",
            "data": {
                "name": "home",
                "navName": "首页"
            },
            "controller": {
                "type": "scripts/routes/home/HomeController"
            }
        },
        "process": {
            "pathto": "/process",
            "data": {
                "name": "process",
                "navName": "赴美流程"
            },
            "subs": ["process-visa"],
            "controller": {
                "type": "scripts/routes/process/ProcessController"
            }
        },
        "process-visa": {
            "pathto": "/process/visa",
            "data": {
                "name": "process-visa",
                "navName": "签证申请"
            },
            "controller": {
                "type": "scripts/routes/process/ProcessController"
            }
        },
        "activity": {
            "pathto": "/activity",
            "data": {
                "name": "activity",
                "navName": "在美活动"
            },
            "controller": {
                "type": "scripts/routes/activity/ActivityController"
            }
        },
        "repatriate": {
            "pathto": "/repatriate",
            "data": {
                "name": "repatriate",
                "navName": "回国服务"
            },
            "controller": {
                "type": "scripts/routes/repatriate/RepatriateController"
            }
        },
        "meet": {
            "pathto": "/meet",
            "data": {
                "name": "meet",
                "navName": "遇见美利坚"
            },
            "controller": {
                "type": "scripts/routes/meet/MeetController"
            }
        },
        "news": {
            "pathto": "/news",
            "data": {
                "name": "news",
                "navName": "新闻资讯"
            },
            "controller": {
                "type": "scripts/routes/news/NewsController"
            }
        },
        "qa": {
            "pathto": "/qa",
            "data": {
                "name": "qa",
                "navName": "赴美回答"
            },
            "controller": {
                "type": "scripts/routes/qa/QaController"
            }
        },
        "about": {
            "pathto": "/about",
            "data": {
                "name": "about",
                "navName": "关于我们"
            },
            "subs": ["about-description"],
            "controller": {
                "type": "scripts/routes/about/AboutController"
            }
        },
        "about-description": {
            "pathto": "/about/description",
            "data": {
                "name": "about-description",
                "navName": "公司概况"
            },
            "controller": {
                "type": "scripts/routes/about/AboutController"
            }
        }
    }
}
