{
    "name": "born",
    "title": "BornInUSA·生美国际-中美直营的赴美生子咨询服务商-赴美生子-南京赴美生子-赴美生子机构-赴美生子月子中心",
    "runtime": {
        "skylarkjs": {
            "version": "0.9.5"
        },
        "paths": {
            "server": "scripts/services/server",
            "photoswipe": "https://cdn.bootcss.com/photoswipe/4.1.2/photoswipe.min",
            "photoswipeUi": "https://cdn.bootcss.com/photoswipe/4.1.2/photoswipe-ui-default.min",
            "skylarkBs": "lib/uncompressed/skylark-bs-swt-all",
            "jquery": "lib/uncompressed/skylark-jquery-all",
            "toastr": "https://cdn.bootcss.com/toastr.js/latest/js/toastr.min",
            "bootstrap": "https://cdn.bootcss.com/bootstrap/3.3.6/js/bootstrap",
            "handlebars": "https://cdn.bootcss.com/handlebars.js/4.0.11/handlebars.amd.min",
            "text": "https://cdn.bootcss.com/require-text/2.0.12/text.min"
        }
    },
    "page": {
        "routeViewer": "#main"
    },
    "spaModule": "scripts/helpers/spa",
    "plugins": {
        "app": {
            "hookers": "starting started",
            "controller": {
                "type": "scripts/plugins/app/AppController"
            }
        },
        "navbar": {
            "hookers": "starting",
            "controller": {
                "type": "scripts/plugins/navbar/NavbarController"
            }
        }
    },
    "routes": {
        "home": {
            "pathto": "/",
            "data": {
                "name": "home",
                "navName": "首页"
            },
            "controller": {
                "type": "scripts/routes/home/HomeController"
            }
        },

        "process": {
            "pathto": "/process",
            "data": {
                "name": "process",
                "navName": "赴美流程"
            },
            "subs": ["process-visa", "process-cohospital", "process-environment", "process-certificate"],
            "controller": {
                "type": "scripts/routes/process/ProcessController"
            }
        },
        "process-visa": {
            "sub": true,
            "pathto": "/process/visa",
            "data": {
                "name": "process-visa",
                "navName": "签证申请"
            },
            "controller": {
                "type": "scripts/routes/process/subs/VisaController"
            }
        },
        "process-environment": {
            "sub": true,
            "pathto": "/process/environment",
            "data": {
                "name": "process-environment",
                "navName": "待产环境"
            },
            "controller": {
                "type": "scripts/routes/process/subs/EnvironmentController"
            }
        },
        "process-cohospital": {
            "sub": true,
            "pathto": "/process/cohospital",
            "data": {
                "name": "process-cohospital",
                "navName": "合作医院"
            },
            "controller": {
                "type": "scripts/routes/process/subs/CohospitalController"
            }
        },
        "process-certificate": {
            "sub": true,
            "pathto": "/process/certificate",
            "data": {
                "name": "process-certificate",
                "navName": "证件办理"
            },
            "controller": {
                "type": "scripts/routes/process/subs/CertificateController"
            }
        },
        "activity": {
            "pathto": "/activity",
            "data": {
                "name": "activity",
                "navName": "在美活动"
            },
            "controller": {
                "type": "scripts/routes/activity/ActivityController"
            }
        },
        "service": {
            "pathto": "/service",
            "data": {
                "name": "service",
                "navName": "回国服务"
            },
            "controller": {
                "type": "scripts/routes/service/ServiceController"
            }
        },
        "metusa": {
            "pathto": "/metusa",
            "data": {
                "name": "metusa",
                "navName": "遇见美利坚"
            },
            "controller": {
                "type": "scripts/routes/metusa/MetusaController"
            }
        },
        "news": {
            "pathto": "/news",
            "data": {
                "name": "news",
                "navName": "新闻资讯"
            },
            "controller": {
                "type": "scripts/routes/news/NewsController"
            }
        },
        "fqa": {
            "pathto": "/fqa",
            "data": {
                "name": "fqa",
                "navName": "赴美回答"
            },
            "controller": {
                "type": "scripts/routes/fqa/FqaController"
            }
        },
        "about": {
            "pathto": "/about",
            "data": {
                "name": "about",
                "navName": "关于我们"
            },
            "subs": ["about-contact"],
            "controller": {
                "type": "scripts/routes/about/AboutController"
            }
        },
        "about-contact": {
            "sub": true,
            "pathto": "/about/contact",
            "data": {
                "name": "about-contact",
                "navName": "联系我们"
            },
            "controller": {
                "type": "scripts/routes/about/subs/ContactController"
            }
        }
    }
}