<div>
    <script id="fqa-show-main" type="text/x-handlebars-template">
        <div class="container">
            <textarea class="hide"></textarea>
            <section class="case_intro">
                <div class="row">
                    <div class="col-12 col-md-9 offset-md-3 col-lg-7 offset-lg-3">
                        <h1 class="display-1">{{title}}</h1>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12 col-md-9 offset-md-3 col-lg-7 offset-lg-3">
                        <img src="{{imgUrl}}" />
                    </div>
                </div>
                <div class="row">
                    <div class="case_info col-12 col-md-3 col-lg-3">
                        <p>{{date}}</p>
                    </div>
                    <div class="col-12 col-md-9 col-lg-7 post_overview"></div>
                </div>
            </section>
        </div>
    </script>
    <script id="qa-listItem-partial" type="text/x-handlebars-template">
        <div class="media panel-body item" data-id="{{id}}">
            <div class="media-left col-md-4">
                <a href="#">
                    <img class="media-object" src="{{src}}" alt="..." style="width:100%;">
                </a>
            </div>
            <div class="media-body media-right">
                <h4 class="media-heading">{{title}}</h4>
                <p class="text-tip"> {{content}}</p>
                <p class="media-footer">
                    <span>{{publishedDate}}</span>
                    <span class="pull-right">
                        <i class="glyphicon glyphicon-eye-open icon"></i>
                        {{viewCount}}
                    </span>
                </p>
            </div>
        </div>
    </script>
</div>
