            },
            "controller": {
                "type": "scripts/routes/courses/CourseShowController"
            }
        },
        "postShow": {
            "hide": true,
            "pathto": "/posts/:id",
            "data": {
                "name": "postShow",
                "navName": "查看"
            },
            "controller": {
                "type": "scripts/routes/metusa/MetusaShowController"
            }
        },
        "news": {
            "pathto": "/news",
            "data": {
                "name": "news",
                "navName": "新闻资讯"
            },
            "controller": {
                "type": "scripts/routes/news/NewsController"
            }
        },
        "newsShow": {
            "pathto": "/news/:id",
            "hide": true,
            "data": {
                "name": "newsShow",
                "navName": "查看新闻"
            },
            "controller": {
                "type": "scripts/routes/news/NewsShowController"
            }
        },
        "fqa": {
            "pathto": "/fqa",
            "data": {
                "name": "fqa",
