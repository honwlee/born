            },
            "controller": {
                "type": "scripts/routes/fqa/FqaController"
            }
        },
        "fqaShow": {
            "hide": true,
            "pathto": "/fqa/:id",
            "data": {
                "name": "fqaShow",
                "navName": "查看回答"
            },
            "controller": {
                "type": "scripts/routes/fqa/FqaShowController"
            }
        },
        "about": {
            "pathto": "/about",
            "data": {
                "name": "about",
                "navName": "关于我们"
            },
            "subs": [
                "about-about",
                "about-contact",
                "about-job"
            ],
            "controller": {
                "type": "scripts/routes/about/AboutController"
            }
        },
        "about-about": {
            "sub": true,
            "pathto": "/about",
            "data": {
                "name": "about-about",
                "navName": "关于我们"
            },
            "controller": {
                "type": "scripts/routes/about/AboutController"
            }
        },
        "about-contact": {
            "sub": true,
            "pathto": "/about/contact",
            "data": {
                "name": "about-contact",
                "navName": "联系我们"
            },
            "controller": {
                "type": "scripts/routes/about/subs/ContactController"
            }
        },
        "about-job": {
            "sub": true,
            "pathto": "/about/job",
            "data": {
                "name": "about-job",
                "navName": "工作机会"
            },
            "controller": {
                "type": "scripts/routes/about/subs/JobController"
            }
        }
    }
}